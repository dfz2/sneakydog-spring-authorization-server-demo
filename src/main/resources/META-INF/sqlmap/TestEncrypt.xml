<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dog.sneaky.demo.database.dao.TestEncryptDAO">
    <select id="list" parameterType="TestEncryptDO" resultType="TestEncryptDO">
        select * from test_encrypt where name = #{name}
    </select>
    <insert id="insert" parameterType="TestEncryptDO" useGeneratedKeys="true" keyProperty="id">
        insert into test_encrypt(name) values (#{name})
    </insert>
</mapper>
