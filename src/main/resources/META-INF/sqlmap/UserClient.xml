<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dog.sneaky.demo.database.dao.UserClientDAO">

    <select id="loadByUsername" parameterType="String" resultType="UserClientDO">
        select * from user_clients where username = #{username} limit 1
    </select>

</mapper>
